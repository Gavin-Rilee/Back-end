{
    "name": "Anywhere-Fitness",
    "version": "1.0.0",
    "description": "Anywhere-Fitness-Back-End",
    "main": "index.js",
    "scripts": {
        "bundle": "knex migrate:rollback && knex migrate:latest && knex seed:run",
        "start": "node index.js",
        "server": "nodemon index.js",
        "migrate": "knex migrate:latest",
        "rollback": "knex migrate:rollback",
        "seed": "knex seed:run",
        "hbundle": "npm run rollbackh && npm run migrateh && npm run seedh",
        "migrateh": "heroku run knex migrate:latest -a tt75-anywhere-fitness",
        "rollbackh": "heroku run knex migrate:rollback -a tt75-anywhere-fitness",
        "seedh": "heroku run knex seed:run -a tt75-anywhere-fitness",
        "test": "cross-env NODE_ENV=testing jest --watchAll --verbose --runInBand"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TT-75-AnyWhereFitness/Back-end"
    },
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/TT-75-AnyWhereFitness/Back-end/issues"
    },
    "homepage": "https://github.com/TT-75-AnyWhereFitness/Back-end#readme",
    "engines": {
        "node": "14.15.4",
        "npm": "6.14.11"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "helmet": "^4.4.1",
        "jsonwebtoken": "^8.5.1",
        "knex": "^0.21.16",
        "pg": "^8.5.1",
        "sqlite3": "^5.0.2"
    },
    "devDependencies": {
        "@types/jest": "^26.0.20",
        "cross-env": "^7.0.3",
        "eslint": "^7.19.0",
        "jest": "^26.6.3",
        "nodemon": "^2.0.7",
        "supertest": "^6.1.3"
    }
}
